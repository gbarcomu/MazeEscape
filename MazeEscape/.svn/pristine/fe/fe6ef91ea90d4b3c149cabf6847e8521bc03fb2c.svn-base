package Robot;

import Otros.Direcciones;

public class Sonny extends RobotRecogedor {
	/** Constructor parametrizado de la clase Sonny que establece el nombre, el identificador y las direcciones a seguir.*/
	public Sonny (String nombre, Direcciones[] direcciones) {
		
		super (nombre, 0, 0, direcciones);
		super.idRobot = 'S';
	}
}