package Robot;

public class Spirit extends RobotRecogedor {
	/** Constructor parametrizado de la clase Spirit que establece el nombre, el identificador y las direcciones a seguir.*/
	public Spirit (String nombre, char marca, int x, int y, int turno) {
		
		super (nombre, x, y, turno);
		super.idRobot = marca;
	}
}
