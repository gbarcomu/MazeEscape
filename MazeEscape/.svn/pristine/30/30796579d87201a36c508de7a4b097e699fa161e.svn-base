package Robot;

import Otros.Direcciones;

public class Bender extends RobotRecogedor {
	/** Constructor parametrizado de la clase Bender que establece el nombre, el identificador y las direcciones a seguir.*/
	public Bender (String nombre, Direcciones[] direcciones) {
		
		super (nombre, 0, 0, direcciones);
		super.idRobot = 'B';
	}
}
